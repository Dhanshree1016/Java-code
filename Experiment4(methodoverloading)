package dhanshree;

public class exp4 {
   void multiply() {
	  int a=2;
	  int b=5;
	  int c=a*b;
	  System.out.println("Multiplication is"+c);
  }
   void multiply(int a) {
	   int c=a*a;
	   System.out.println("Multiplication is"+c);
   }
   static void multiply(int a,int b) {
	   int c=a*b;
	   System.out.println("Multiplication is"+c);
   }
   public static void main(String[]args) {
	   int a=5,b=10;
	   exp4 obj=new exp4();
	   obj.multiply();
	   exp4 obj1=new exp4();
	   obj1.multiply(a);
	   exp4.multiply(a, b);
	   
   }
}
Output is:
Multiplication is10
Multiplication is25
Multiplication is50
